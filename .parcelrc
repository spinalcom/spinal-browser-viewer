{
  "extends": [
    "@parcel/config-default",
    "parcel-config-vue2"
  ],
  "transformers": {
    "*.{ts,tsx}": [
      "@parcel/transformer-typescript-tsc"
    ]
  },
  "reporters": [
    "parcel-reporter-static-files-copy"
  ],
  "compressors": {
    "*.{html,css,js,svg,map}": [
      "...",
      "@parcel/compressor-gzip"
    ]
  }
}