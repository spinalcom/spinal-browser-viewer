{
  "name": "spinal-browser-viewer",
  "version": "v1.0.3",
  "description":
    "A spinalcore browser to visualise a 3D model generated via Autodesk Forge",
  "author": "laurent <htd.laurent@gmail.com>",
  "private": true,
  "scripts": {
    "watch": "node bin/watch.js",
    "dev": "npm run watch",
    "build": "node bin/build.js",
    "postinstall": "npm run spinal-register",
    "spinal-register": "spinal-register --browser"
  },
  "bin": {
    "create_viewer_env": "create_viewer_env.js"
  },
  "dependencies": {
    "spinal-browser-drive":
      "git+https://github.com/spinalcom/spinal-browser-drive.git",
    "spinal-lib-forge-env":
      "git+https://github.com/spinalcom/spinal-lib-forge-env.git",
    "spinal-lib-forgefile":
      "git+https://github.com/spinalcom/spinal-lib-forgefile.git",
    "spinal-organ-forge":
      "git+https://github.com/spinalcom/spinal-organ-forge.git#develop",
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "chart.js": "^2.7.2",
    "envify": "^3.4.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "material-design-icons-iconfont": "^3.0.3",
    "q": "^1.5.1",
    "rxjs": "^5.5.10",
    "sassify": "^4.0.1",
    "spinal-core-connectorjs":
      "git+https://github.com/spinalcom/spinal-core-connectorjs.git",
    "uglify-js": "^2.5.0",
    "vue": "^2.0.1",
    "vue-chartjs": "^3.2.1",
    "vue-material": "^1.0.0-beta-7",
    "vue-rx": "^5.0.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0",
    "commander": "^2.13.0",
    "exorcist": "^1.0.0",
    "windowify": "^0.3.0",
    "uglifyify": "^4.0.5",
    "babel-plugin-transform-runtime": "^6.23.0"
  },
  "devDependencies": {
    "browserify-css": "^0.14.0",
    "browserify-hmr": "^0.3.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "spinal-core-connectorjs":
      "git+https://github.com/spinalcom/spinal-core-connectorjs.git"
  },
  "browserify": {
    "transform": ["vueify", "babelify"]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  }
}
